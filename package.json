{
  "name": "coc-post",
  "version": "0.3.0",
  "description": "post extension for coc.nvim",
  "keywords": [
    "coc.nvim",
    "post",
    "postman",
    "request",
    "api"
  ],
  "main": "./out/index.js",
  "repository": "git@github.com:iamcco/coc-post.git",
  "author": "iamcco <ooiss@qq.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "coc": "^0.0.67"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "post.do",
        "title": "do request of current post file",
        "category": "post"
      },
      {
        "command": "post.new",
        "title": "create a new post file",
        "category": "post"
      }
    ],
    "configuration": {
      "title": "enable",
      "properties": {
        "post.enable": {
          "type": "boolean",
          "default": true,
          "description": "enable this extension?"
        },
        "post.detect": {
          "type": "boolean",
          "default": true,
          "description": "is enable detect filetype for *.post file"
        },
        "post.root": {
          "type": "string",
          "default": "",
          "description": "directory to save post file with post.new command"
        },
        "post.agent": {
          "type": "string",
          "default": "",
          "description": "Proxy agent, format like: http://168.63.76.32:3128"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.3.4",
    "@types/pretty-hrtime": "^1.0.0",
    "coc.nvim": "^0.0.77",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "https-proxy-agent": "^5.0.0",
    "node-fetch": "^2.6.0",
    "pretty-hrtime": "^1.0.3",
    "vscode-languageserver-protocol": "^3.15.3"
  }
}
